{
    "compilerOptions": {
        "outDir": "dist",
        "target": "es2015",
        "moduleResolution": "node",
        "module": "commonjs",
        "lib": ["es2015", "es2017"],
        "sourceMap": true,
        "strict": true,
        "noImplicitAny": false,
        "noUnusedLocals": true,
        "downlevelIteration": true,
        "experimentalDecorators": true
    }
}